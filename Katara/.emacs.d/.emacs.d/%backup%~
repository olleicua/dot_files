<VirtualHost *:80>
    ServerName www.pst.dev
    DocumentRoot /home/olleicua/Work/greenriver/pst-goputney/wordpress
    <Directory /home/olleicua/Work/greenriver/pst-goputney/wordpress/>
        Options +FollowSymLinks +Indexes
        Order allow,deny
        Allow from all
        AllowOverride All
        Require all granted
    </Directory>
</VirtualHost>
<VirtualHost *:80>
    ServerName app.pst.dev
    DocumentRoot /home/olleicua/Work/greenriver/pst-goputney/site/web/app
    SetEnv SYMFONY_ENV dev
    SetEnv SYMFONY_DEBUG false
    SetEnv SYMFONY_DISABLE_SSL true
    <Directory /home/olleicua/Work/greenriver/pst-goputney/site/web/app/>
        Order allow,deny
        Allow from all
        Options +FollowSymLinks +Indexes
        AllowOverride All
        Require all granted
    </Directory>
</VirtualHost>
